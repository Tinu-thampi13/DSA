#include <bits/stdc++.h>
using namespace std;

class Solution{
public:	

	int print2largest(int arr[], int n) {
     int firstlargest=arr[0];
    int secondlargest=-1;
    for(int i=1;i<n;i++){
        if(arr[i]>firstlargest){
            secondlargest=firstlargest;
            firstlargest=arr[i];
            
           
        }
        else if(arr[i]<firstlargest&&arr[i]>secondlargest){
            secondlargest=arr[i];
        }
        
    }
    return secondlargest;
	}
};


int main() {
    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;
        int arr[n];
        for (int i = 0; i < n; i++) {
            cin >> arr[i];
        }
        Solution ob;
        auto ans = ob.print2largest(arr, n);
        cout << ans << "\n";
    }
    return 0;
}
